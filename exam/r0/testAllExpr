main{
    print 1+1;
    print 1-1;
    print 1*1;
    print 1/1;

    print 1<1;
    print 1>1;
    print 1<=1;
    print 1>=1;
    print 1==1;
    print 1!=1;

    real realExpr = (10+6)/(10/2)+(-2);
    print realExpr;

    bool booleanExpr = (10+4)-2<(5*50-1);
    print booleanExpr

    print (1);
    print (1==1)||(false);

    print (1<9)||(not (2+2)<(4/4));
    print (1<9)and(not (2+2)<(4/4));
    print (not (2+2)<(4/4));
}
